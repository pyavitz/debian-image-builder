+static unsigned char ssd1306_set_display_type(struct vfd_display *display)
+{
+	unsigned char ret = 0;
+	if (display->controller == CONTROLLER_SSD1306 || display->controller == CONTROLLER_SH1106) {
+		dev->dtb_active.display = *display;
+		ssd1306_init();
+		ret = 1;
+	}
+
+	return ret;
+}
