+}
+
+static unsigned char i2c_sw_read_raw_byte(unsigned char *data)
+{
+	unsigned char i = 8;
+	unsigned char mask = lsb_first ? 0x80 : 0x01;
+	*data = 0;
+	gpio_set_pin_high(&pin_sda);
+	while (i--) {
+		if (lsb_first)
+			*data >>= 1;
+		else
+			*data <<= 1;
+		gpio_set_pin_high(&pin_scl);
+		udelay(i2c_sw_delay);
+		if (gpio_get_value(pin_sda.pin))
