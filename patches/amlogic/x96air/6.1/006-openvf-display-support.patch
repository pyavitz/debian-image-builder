+	return 1;
+}
+
+static unsigned char pcd8544_set_display_type(struct vfd_display *display)
+{
+	unsigned char ret = 0;
+	if (display->controller == CONTROLLER_PCD8544) {
+		dev->dtb_active.display = *display;
+		pcd8544_init();
+		ret = 1;
+	}
+
+	return ret;
+}
