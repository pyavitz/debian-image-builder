+static int is_right_chip(struct gpio_chip *chip, void *data)
+{
+	pr_dbg("is_right_chip %s | %s | %d\n", chip->label, (char*)data, strcmp(data, chip->label));
+	if (strcmp(data, chip->label) == 0)
+		return 1;
+	return 0;
