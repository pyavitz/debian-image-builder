+static unsigned int openvfd_dev_poll(struct file *filp, poll_table * wait)
+{
+	unsigned int mask = 0;
+	struct vfd_dev *dev = filp->private_data;
+	poll_wait(filp, &dev->kb_waitq, wait);
+	if (dev->key_respond_status)
+		mask |= POLLIN | POLLRDNORM;
+	return mask;
+}
