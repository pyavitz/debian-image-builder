+					};
+
 					pwm_d_x3_pins: pwm-d-x3 {
 						mux {
 							groups = "pwm_d_x3";
@@ -892,6 +916,14 @@ mux {
 						};
 					};
 
+					pwm_d_z2_pins: pwm-d-z2 {
+						mux {
+							groups = "pwm_d_z2";
+							function = "pwm_d";
+							bias-disable;
+						};
+					};
+
 					pwm_e_pins: pwm-e {
 						mux {
 							groups = "pwm_e";
@@ -1940,6 +1972,33 @@ mux {
 							bias-disable;
 						};
 					};
+					
+					uart_ao_b_tx_2_rx_3_pins: uart-ao-b-tx-2-rx-3 {
+						mux {
+							groups = "uart_ao_b_tx_2",
+								"uart_ao_b_rx_3";
+							function = "uart_ao_b";
+							bias-disable;
+						};
+					};
+
+					uart_ao_b_tx_8_rx_9_pins: uart-ao-b-tx-8-rx-9 {
+						mux {
+							groups = "uart_ao_b_tx_8",
+								"uart_ao_b_rx_9";
+							function = "uart_ao_b";
+							bias-disable;
+						};
+					};
+
+					uart_ao_b_cts_rts_pins: uart-ao-b-cts-rts {
